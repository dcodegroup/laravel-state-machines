{
    "name": "dcodegroup/state-machines",
    "description": "State machines for Laravel",
    "license": "MIT",
    "keywords": [
        "state-machines",
        "laravel"
    ],
    "authors": [
        {
            "name": "Yohann Catherine",
            "email": "yohann@dcodegroup.com",
            "homepage": "https://dcodegroup.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/dcodegroup/state-machines",
    "require": {
        "php": "^8.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Dcodegroup\\StateMachines\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dcodegroup\\StateMachines\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    }
}