{
    "name": "dcodegroup/state-machines",
    "description": "Generates state machine classes for Laravel",
    "license": "MIT",
    "keywords": [
        "state-machines",
        "laravel"
    ],
    "authors": [
        {
            "name": "Yohann Catherine",
            "email": "yohann@dcodegroup.com",
            "homepage": "https://dcodegroup.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/dcodegroup/state-machines",
    "require": {
        "php": "^8.2",
        "illuminate/console": "^10.0",
        "illuminate/database": "^10.0",
        "illuminate/support": "^10.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Dcodegroup\\StateMachines\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dcodegroup\\StateMachines\\StateMachineServiceProvider"
            ]
        }
    }
}